
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id     Int     @id @default(autoincrement())
  name   String?
  password String?
  nome String?
  idade String?
  data_nascimento String?
  curso String?
  lingua_1 String?
  lingua_2 String?
  lingua_3 String?
  lingua_4 String?
  trabalho_1 String?
  trabalho_2 String?
  habilidade_1 String?
  habilidade_2 String?
  habilidade_3 String?
  habilidade_4 String?
}
